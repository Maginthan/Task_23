\f0\b\fs24 \cf0 #Query for creating a database and using the database
\f1\b0 \
create database employeeDatabase;\
Use employeeDatabase;\
\

\f0\b #Query for creating a table inside the above created database
\f1\b0 \
create table employeeTable(empno int,ename varchar(25),job varchar(25),mgr int,hiredate DATE,sal DECIMAL(8,2),comm DECIMAL(8,2),deptno int);\
