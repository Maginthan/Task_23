\f0\b #Query for displaying all records in the table
\f1\b0 \
select * from employeeTable;\
\

\f0\b # A) Query to display EName and Sal of employees whose salary is greater than or equal to 2200
\f1\b0 \
select ename,sal from employeeTable where sal>=2200;\
\

\f0\b # B) Query to display details of employees who are not getting commission
\f1\b0 \
select * from employeeTable where comm is null;\
\

\f0\b # C) Query to display employee name and salary of employees who don't have salary in the range of 2500 to 4000
\f1\b0 \
select ename,sal from employeeTable where sal not between 2500 and 4000;\
\

\f0\b # D) Query to display name, job title and salary of employees who don't have a manager
\f1\b0 \
select ename,job,sal from employeeTable where mgr is null;\
\

\f0\b # E) Query to display the employee name whose name contains "A" as third alphabet
\f1\b0 \
select ename from employeeTable where ename like '__A%';\
\

\f0\b # F) Query to display the employee name whose name contains "T" as the last alphabet
\f1\b0 \
select ename from employeeTable where ename like '%T';}